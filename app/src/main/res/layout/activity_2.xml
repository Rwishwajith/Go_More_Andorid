<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    android:id="@+id/drawer"
    tools:context=".Activity2">

    <LinearLayout
        android:layout_width="match_parent"
        android:background="@drawable/df"
        android:layout_height="match_parent">


        <ViewFlipper
            android:layout_width="match_parent"
            android:layout_height="250dp"
            android:layout_centerHorizontal="true"
            android:id="@+id/v_flipper"/>


    </LinearLayout>

    <Button
        android:id="@+id/btnSearch"
        android:layout_width="match_parent"
        android:layout_height="30sp"
        android:layout_marginTop="250dp"
        android:layout_marginBottom="330dp"
        android:text="Search"
        android:background="@color/darkgray"
        android:textColor="@color/white"/>

    <Button
        android:id="@+id/btn1"
        android:layout_width="match_parent"
        android:layout_height="20sp"
        android:layout_marginTop="330dp"
        android:layout_marginRight="295dp"
        android:layout_marginBottom="185dp"
        android:background="@drawable/calendar"
        android:text="Booking"
        android:textColor="@android:color/black" />

    <Button
        android:id="@+id/btn2"
        android:layout_width="match_parent"
        android:layout_height="20sp"
        android:layout_marginRight="0dp"
        android:layout_marginLeft="280dp"
        android:layout_marginTop="330dp"
        android:layout_marginBottom="185dp"
        android:background="@drawable/pickup"
        android:text="Pick Where"
        android:textColor="@android:color/black"
        />

    <Button
        android:id="@+id/btn3"
        android:layout_width="match_parent"
        android:layout_height="20sp"
        android:layout_marginLeft="130dp"
        android:layout_marginTop="330dp"
        android:layout_marginRight="140dp"
        android:layout_marginBottom="185dp"
        android:background="@drawable/store"
        android:text="Store"
        android:textColor="@android:color/black"/>

    <Button
        android:id="@+id/btn4"
        android:layout_width="match_parent"
        android:layout_height="20sp"
        android:layout_marginLeft="130dp"
        android:layout_marginTop="520dp"
        android:layout_marginRight="140dp"
        android:layout_marginBottom="0dp"
        android:background="@drawable/placeholder"
        android:text="Popular Place"
        android:textColor="@android:color/black"/>

    <Button
        android:id="@+id/btn5"
        android:layout_width="match_parent"
        android:layout_height="20sp"
        android:layout_marginTop="520dp"
        android:layout_marginRight="295dp"
        android:layout_marginBottom="0dp"
        android:background="@drawable/mona"

        android:text="Gallary"
        android:textColor="@android:color/black" />

    <Button
        android:id="@+id/btn6"
        android:layout_width="match_parent"
        android:layout_height="20sp"
        android:layout_marginRight="0dp"
        android:layout_marginLeft="280dp"
        android:layout_marginTop="520dp"
        android:layout_marginBottom="0dp"
        android:background="@drawable/phone"
        android:text="Contact Us"
        android:textColor="@android:color/black"
        />




    <android.support.design.widget.NavigationView
        app:headerLayout="@layout/header"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        app:itemIconTint="@color/darkgray"
        app:itemTextColor="@color/darkgray"
        app:menu="@menu/drawermenu"
        android:layout_gravity="start" >






    </android.support.design.widget.NavigationView>

